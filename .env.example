# Edge Agent Environment Variables
# Copy this file to .env and fill in your values

# Shared secret for HMAC authentication (must match backend)
EDGE_SECRET=your_shared_secret_here

# Registration token for initial setup
REGISTRATION_TOKEN=edge_initial_token

# Backend URL (override config.yaml if needed)
# BACKEND_URL=https://archety-backend.onrender.com

# User phone number (override config.yaml if needed)
# USER_PHONE=+15551234567
